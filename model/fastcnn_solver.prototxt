net: "fastcnn_train_val.prototxt"
test_iter: 200
test_interval: 5000
base_lr: 0.0001
momentum: 0.9
weight_decay: 0.0002
# The learning rate policy
lr_policy: "fixed"
# gamma: 0.0001
# power: 0.75
# Display every 100 iterations
display: 100
# The maximum number of iterations
max_iter: 200000
# snapshot intermediate results
snapshot: 5000
snapshot_prefix: "fastcnn"
# solver mode: CPU or GPU
solver_mode: GPU
